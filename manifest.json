{
  "manifest_version": 3,
  "name": "Web Scraper Extension",
  "version": "1.0",
  "description": "A Chrome extension to scrape text data from a website and download it.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"

  },
  "options_page": "options.html"
}
